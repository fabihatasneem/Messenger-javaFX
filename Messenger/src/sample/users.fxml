<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="layout" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UsersController">
   <children>
      <Label fx:id="title" layoutX="173.0" layoutY="14.0" text="Find your friends here!">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="logoutButton" layoutX="355.0" layoutY="348.0" mnemonicParsing="false" onAction="#logoutAction" text="Log Out" />
      <Button fx:id="chatButton" layoutX="171.0" layoutY="346.0" mnemonicParsing="false" onAction="#chatAction" prefHeight="35.0" prefWidth="56.0" text="Chat!" />
      <ListView fx:id="listView" layoutX="172.0" layoutY="67.0" prefHeight="253.0" prefWidth="258.0" />
      <Button fx:id="refreshButton" layoutX="21.0" layoutY="16.0" mnemonicParsing="false" onAction="#refreshAction" text="Refresh" />
   </children>

</AnchorPane>
